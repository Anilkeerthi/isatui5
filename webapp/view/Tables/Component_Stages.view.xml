<mvc:View
    controllerName="com.isat.isatui5.controller.Tables.Component_Stages"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:tnt="sap.tnt"
    xmlns="sap.m"
    xmlns:m="sap.m"
    xmlns:uxap="sap.uxap"
>
    <mvc:content>
        <tnt:ToolHeader
            id="shellLikeToolHeaderComponent_Stages"
            class="sapUiTinyMargin"
        >
            <SearchField
                width="auto"
                class="searchFieldCustomWidth"
                id="searchFieldComponent_Stages"
                placeholder="Search here"
            >
                <layoutData>
                    <OverflowToolbarLayoutData
                        priority="NeverOverflow"
                        group="1"
                    />
                </layoutData>
            </SearchField>

            <ToolbarSpacer />
            <Button
                text="NEW"
                tooltip="New"
                press="onComponent_StagesNew"
            />
            <Button
                text="Import"
                tooltip="Import"
            />
            <Button
                text="Export"
                tooltip="Export"
            />
        </tnt:ToolHeader>

        <ToolbarSpacer />

        <Table
            id="idComponent_Stages"
            inset="false"
            items="{
			path: 'jsonModel>/Component_Stages'
		
		}"
        >
            <infoToolbar>
                <OverflowToolbar>
                    <Label text="Component_Stages" />
                </OverflowToolbar>
            </infoToolbar>
            <columns>
                <Column width="12em">
                    <Text text="AutoID" />
                </Column>
                <Column
                    minScreenWidth="Tablet"
                    demandPopin="true"
                >
                    <Text text="StageID_Name" />
                </Column>
                <Column
                    minScreenWidth="Desktop"
                    demandPopin="true"
                    hAlign="Center"
                >
                    <Text text="Status" />
                </Column>
                <Column hAlign="Begin" width="12em">
                    <Text text="Actions" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle">
                    <cells>
                        <ObjectIdentifier text="{jsonModel>autoid}"/>
                        <ObjectIdentifier text="{jsonModel>stage_id/name}"/>
                        <ObjectIdentifier text="{jsonModel>status}"/>

                            <Button
                                type="Emphasized"
                                press=".onShowDetails"
                                icon="sap-icon://display"
                            /> 
                    </cells>
                </ColumnListItem>
            </items>
        </Table>

        <!-- Dialog Box for New -->

        <Dialog
            id="Component_StagesDialogNew"
            stretch="false"
            resizable="true"
            draggable="true"
            afterClose="onDialogClose"
        >
            <customHeader>
                <Toolbar>
                    <Title text="Add New Component_Stages Data" />
                    <ToolbarSpacer />
                    <Button
                        icon="sap-icon://decline"
                        press="onCloseDialog"
                        tooltip="Close"
                    />
                </Toolbar>
            </customHeader>
            <content>
                <HBox class="adminScreenActions">
                    <VBox>
                    <Label
                            text="DDType
                             Name"
                            required="true"/>
                      <ComboBox
                            id="Component_StagesDialogNewComboBox"
                            change="onComponent_StagesDialogNewComboBox"
                            placeholder="All"
                            width="100%"
                            items="{
                                     path: 'jsonModel>/Stages'
                   
                                    }"
                        >
                            <core:Item
                                key="{jsonModel>autoid}"
                                text="{jsonModel>name}"
                            />
                        </ComboBox>
                    </VBox>

                    <VBox>
                        <Label
                            text="Status:"
                            required="true"
                        />
                        <Input
                            value=""
                            id="Component_StagesStatus"
                        />
                    </VBox>
                </HBox>

               
            </content>

            <beginButton>
                <Button
                    text="Save"
                    tooltip="Save"
                    press="onSaveComponent_StagesNew"
                />
            </beginButton>
            <endButton>
                <Button
                    text="Clear"
                    tooltip="Clear"
                    press="onClearDialog"
                />
            </endButton>
        </Dialog>

        <!-- Dialog Box for Actions -->

        <Dialog
            id="Component_StagesDialog"
            stretch="false"
            resizable="true"
            draggable="true"
            afterClose="onDialogClose"
        >
            <customHeader>
                <Toolbar>
                    <Title text="ComponentStages Details" />
                    <ToolbarSpacer />
                    <Button
                        icon="sap-icon://decline"
                        press="onCloseDialogAction"
                    />
                </Toolbar>
            </customHeader>
            <content>
                <HBox class="adminScreenActions">
                    <VBox>
                        <Label text="AutoID:" />
                        <Input
                            value="{dialogModel>/autoid}"
                            editable="false"
                        />
                    </VBox>

                     <VBox>
                        <Label text="Status:" />
                        <Input
                            value="{dialogModel>/status}"
                            editable="{dialogModel>/editable}"
                        />
                    </VBox>
                </HBox>

                <HBox class="adminScreenActions">
                    <VBox>
                        <Label text="StageID_Name:" />
                        <Input
                            value="{dialogModel>/stage_id/name}"
                             editable="false"
                        />
                    </VBox>
                </HBox>
            </content>

           <beginButton>
                <Button
                    text="{dialogModel>/buttonText}"
                    press="onToggleEdit"
                />
            </beginButton>
            <endButton>
                <Button
                     text="{dialogModel>/deleteButtonText}"
                    press="onDeletePress"
                />
            </endButton>
        </Dialog>
    </mvc:content>
</mvc:View>
